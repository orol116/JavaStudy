	<!-- 회원 정보 수정 -->
	<update id="updateInfo">
		UPDATE MEMBER_S SET
		MEMBER_NICK = #{updateNickname},
		MEMBER_TEL = #{updateTel},
		MEMBER_ADDR = #{memberAddress}
		WHERE MEMBER_NO = #{memberNo}
	</update>

